@model  Link
@{
    ViewData["Title"] = "RemoveLinks";
}

<h1>RemoveLinks</h1>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Register">
            <div>
                <label>Select Link</label>
                @Html.DropDownListFor(x => x.Roles.Id, Model.selectLists, "Select", new { @class = "form-control" })
            </div>
          
            <div class="form-group">
                <input type="submit" value="Remove Link" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
